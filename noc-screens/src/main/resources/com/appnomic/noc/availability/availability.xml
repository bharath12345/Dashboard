<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<!-- The package element provides a mechanism by which to group related 
		Struts 2 components, such as actions and results, into logical units. -->
	<package name="availability" namespace="/availability" extends="json-default">

		<action name="AvailabilityMeta" class="com.appnomic.noc.availability.AvailabilityMetaAction">
			<result type="json"></result>
		</action>

		<action name="AvailabilityRequestHandler" class="com.appnomic.noc.availability.AvailabilityRequestHandlerAction">
			<result type="json"></result>
		</action>

	</package>

</struts>
