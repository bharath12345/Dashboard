<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- The package element provides a mechanism by which to group related 
		Struts 2 components, such as actions and results, into logical units. -->
	<package name="noc" namespace="/noc" extends="json-default">
		<action name="RequestHandler" class="com.appnomic.noc.action.RequestHandlerAction">
			<result type="json"></result>
		</action>
	</package>
</struts>
